<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.josh.alc40phase.sec">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name="com.josh.alc40phase.sec.site.josh.alc40phase.sec.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:testOnly="false"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.josh.alc40phase.sec.site.josh.alc40phase.sec.AuthActivity"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.josh.alc40phase.sec.site.josh.alc40phase.sec.AdminActivity"
            android:parentActivityName="com.josh.alc40phase.sec.site.josh.alc40phase.sec.UserActivity"
            android:label="@string/app_name"
            />
        <activity android:name="com.josh.alc40phase.sec.site.josh.alc40phase.sec.UserActivity"
            android:launchMode="singleInstance"
            android:label="@string/app_name"/>
    </application>

</manifest>